{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/(.*)",
        "methods": ["GET,HEAD,PUT,PATCH,POST,DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
          
        }
      }
    ]
  }
